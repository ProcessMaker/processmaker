{
  "name": "processmaker/bpm",
  "version": "4.0.0",
  "description": "BPM PHP Software",
  "keywords": [
    "php bpm processmaker"
  ],
  "homepage": "http://processmaker.com",
  "license": "GNU Affero General Public License version 3",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/ProcessMaker/laravel-chunk-upload"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "TYPO3/class-alias-loader": "^1.0",
    "darkaonline/l5-swagger": "5.7.*",
    "doctrine/dbal": "^2.5",
    "fideloper/proxy": "^4.0",
    "igaster/laravel-theme": "2.0.*",
    "laravel/framework": "5.7.*",
    "laravel/horizon": "^1.3",
    "laravel/passport": "^7.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "^5.4.0",
    "lavary/laravel-menu": "^1.7",
    "phing/phing": "^2.16",
    "pion/laravel-chunk-upload": "dev-bugfix",
    "predis/predis": "^1.1",
    "processmaker/nayra": "dev-master",
    "ralouphie/getallheaders": "^2.0",
    "spatie/laravel-fractal": "^5.3",
    "spatie/laravel-medialibrary": "^7.0.0",
    "symfony/expression-language": "^4.1",
    "symfony/psr-http-message-bridge": "1.*",
    "watson/validating": "^3.1",
    "zendframework/zend-diactoros": "1.*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/dusk": "^3.0",
    "laravel/homestead": "^7.18",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "autoload": {
   "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "ProcessMaker\\": "ProcessMaker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk",
        "laravel/passport"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
