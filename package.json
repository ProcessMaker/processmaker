  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "hot": "mix watch --hot",
    "lint": "eslint --fix --ignore-path .eslintignore resources/",
    "postinstall": "mkdir -p public/css/precompiled && cp -rf node_modules/npm-font-open-sans public/css/precompiled/npm-font-open-sans && cp -rf node_modules/bootstrap/scss public/css/precompiled/bootstrap && cp -rf node_modules/@fortawesome/fontawesome-free public/css/precompiled/fontawesome-free && cp -rf node_modules/@processmaker/vue-multiselect/dist/vue-multiselect.min.css public/css/precompiled",
    "prod": "npm run production",
    "production": "mix --production",
    "test": "cross-env NODE_ENV=test jest",
    "test-watch": "npm run test -- --watch --notify",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000"
  },
